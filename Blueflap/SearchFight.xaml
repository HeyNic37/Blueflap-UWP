                                        <ProgressBar Height="4" Margin="10,0,70,0" VerticalAlignment="Center" Foreground="#FF787878" Background="{x:Null}" IsIndeterminate="True"/>
                                        <Button Content="&#xE1CD;" HorizontalAlignment="Right" Margin="0" VerticalAlignment="Stretch" Background="#00000000" Width="50" FontFamily="Segoe MDL2 Assets" Tapped="Button_Tapped_2"/>
                                    </Grid>
                                    <StackPanel x:Name="SE_Custom_List"/>
                                </StackPanel>
                            </ScrollViewer>
                        </Grid>
                    </PivotItem>
                </Pivot>
                <Grid Height="60" Margin="0" VerticalAlignment="Bottom" CornerRadius="2" Background="#26858585">
                    <TextBox x:Name="Popup_Adressbox" x:Uid="FightTextbox" Margin="20,0,49,0" TextWrapping="Wrap" Text="" VerticalAlignment="Center" BorderThickness="1" KeyDown="TextBox_KeyDown"/>
                    <Button Content="&#xE11A;" HorizontalAlignment="Right" Height="32" Margin="0,0,20,0" VerticalAlignment="Center" Width="32" BorderBrush="{ThemeResource BackButtonDisabledForegroundThemeBrush}" BorderThickness="1" Background="{ThemeResource ContentDialogBorderThemeBrush}" Foreground="White" FontFamily="Segoe MDL2 Assets" FontSize="13.333" RenderTransformOrigin="0.5,0.5" Tapped="Button_Tapped_1">
                        <Button.RenderTransform>
                            <CompositeTransform/>
                        </Button.RenderTransform>
                    </Button>
                </Grid>
                <Grid x:Name="Suggest_Copy" Height="48" Margin="20,0,20,52" VerticalAlignment="Bottom" BorderBrush="{ThemeResource ContentDialogBorderThemeBrush}" Background="White" BorderThickness="1" RenderTransformOrigin="0.5,0.5" Visibility="Collapsed">
                    <Grid.RenderTransform>
                        <CompositeTransform/>
                    </Grid.RenderTransform>
                    <TextBlock HorizontalAlignment="Left" Margin="10,15,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontFamily="Segoe MDL2 Assets" FontSize="14.667" Foreground="#FF767676" Text="&#xE81C;"/>
                    <TextBlock x:Uid="LastSearch" HorizontalAlignment="Left" Margin="35,3,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF919191" Text="Dernière recherche" FontSize="13.333"/>
                    <TextBlock x:Name="DerniereRecherche1" Margin="36,20,0,0" TextWrapping="NoWrap" VerticalAlignment="Top" Foreground="#FF535353" Text="BLUEFLAP" FontSize="14" FontWeight="SemiBold" Height="22"/>
                </Grid>
            </Grid>
        </Grid>

    </Grid>
</Page>
