            </Grid.RenderTransform>
            <ScrollViewer Margin="0,0,0,2" VerticalScrollBarVisibility="Auto">
                <StackPanel x:Name="Ellipsis_Content" RenderTransformOrigin="0.5,0.5">
                    <StackPanel.RenderTransform>
                        <CompositeTransform ScaleY="-1"/>
                    </StackPanel.RenderTransform>
                    <Button x:Name="Settings_Button_M" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="40" RenderTransformOrigin="0.5,0.5" Margin="-10,5" Background="#00000000" RequestedTheme="Dark" BorderThickness="0">
                        <Button.RenderTransform>
                            <CompositeTransform ScaleY="-1"/>
                        </Button.RenderTransform>
                        <Grid UseLayoutRounding="True" ScrollViewer.VerticalScrollBarVisibility="Disabled">
                            <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" x:Uid="Menu_Settings" Text="Paramètres" VerticalAlignment="Center" RequestedTheme="Dark"/>
                            <TextBlock HorizontalAlignment="Center" Margin="0,0,200,0" TextWrapping="Wrap" Text="&#xE115;" VerticalAlignment="Center" RequestedTheme="Dark" FontFamily="Segoe MDL2 Assets"/>
                        </Grid>
                    </Button>
                    <Button x:Name="Lock_Button_M" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="40" RenderTransformOrigin="0.5,0.5" Margin="-10,0,-10,5" Background="#00000000" RequestedTheme="Dark" BorderThickness="0">
                        <Button.RenderTransform>
                            <CompositeTransform ScaleY="-1"/>
                        </Button.RenderTransform>
                        <Grid UseLayoutRounding="True" ScrollViewer.VerticalScrollBarVisibility="Disabled">
                            <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" x:Uid="Menu_Lock" Text="Verrouiller" VerticalAlignment="Center" RequestedTheme="Dark"/>
                            <TextBlock HorizontalAlignment="Center" Margin="0,0,200,0" TextWrapping="Wrap" Text="&#xE1F6;" VerticalAlignment="Center" RequestedTheme="Dark" FontFamily="Segoe MDL2 Assets"/>
                        </Grid>
                    </Button>
                    <Button x:Name="Fight_Button_M" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="40" RenderTransformOrigin="0.5,0.5" Margin="-10,0,-10,5" Background="#00000000" RequestedTheme="Dark" BorderThickness="0">
                        <Button.RenderTransform>
                            <CompositeTransform ScaleY="-1"/>
                        </Button.RenderTransform>
                        <Grid UseLayoutRounding="True" ScrollViewer.VerticalScrollBarVisibility="Disabled">
                            <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" Text="Search Fight" VerticalAlignment="Center" RequestedTheme="Dark"/>
                            <TextBlock HorizontalAlignment="Center" Margin="0,0,200,0" TextWrapping="Wrap" Text="&#xE923;" VerticalAlignment="Center" RequestedTheme="Dark" FontFamily="Segoe MDL2 Assets"/>
                        </Grid>
                    </Button>
                    <Button x:Name="Customization_Button_M" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="40" RenderTransformOrigin="0.5,0.5" Margin="-10,0,-10,5" Background="#00000000" RequestedTheme="Dark" BorderThickness="0">
                        <Button.RenderTransform>
                            <CompositeTransform ScaleY="-1"/>
                        </Button.RenderTransform>
                        <Grid UseLayoutRounding="True" ScrollViewer.VerticalScrollBarVisibility="Disabled">
                            <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" x:Uid="Menu_Customize" Text="Personnalisation" VerticalAlignment="Center" RequestedTheme="Dark"/>
                            <TextBlock HorizontalAlignment="Center" Margin="0,0,200,0" TextWrapping="Wrap" Text="&#xE2B1;" VerticalAlignment="Center" RequestedTheme="Dark" FontFamily="Segoe MDL2 Assets"/>
                        </Grid>
                    </Button>
                </StackPanel>
            </ScrollViewer>
        </Grid>
        <Grid x:Name="PhoneNavBar" Height="50" Margin="0" VerticalAlignment="Bottom" Background="White" BorderBrush="#FF939393" BorderThickness="0,1,0,0">
            <Grid x:Name="Ellipsis_Button" HorizontalAlignment="Right" Height="50" Margin="0" VerticalAlignment="Bottom" Width="50" Background="{ThemeResource ContentDialogBorderThemeBrush}" Tapped="Grid_Tapped_1">
                <TextBlock HorizontalAlignment="Center" Margin="1,2,0,0" TextWrapping="Wrap" Text="&#xE10C;" VerticalAlignment="Center" FontFamily="Segoe MDL2 Assets" RequestedTheme="Dark" FontSize="16"/>
            </Grid>
            <ProgressBar x:Name="mobileloader" Height="1" Margin="0,-1,50,0" VerticalAlignment="Top" Background="{x:Null}" Value="20" IsIndeterminate="True" Visibility="Collapsed"/>
            <Button x:Name="Phone_Forward" Content="&#xE0D5; &#xE12B;" HorizontalAlignment="Left" Margin="0,0,50,0" VerticalAlignment="Stretch" Width="64" FontFamily="Segoe MDL2 Assets" Background="#00787878" BorderThickness="1,0,0,0"/>
        </Grid>
    </Grid>
</Page>
